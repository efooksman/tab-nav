{
  "manifest_version": 3,
  "name": "Tab Navigator",
  "version": "1.0",
  "description": "Quick tab switching: Alt+Z to switch to most recent tab",
  "permissions": [
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },


  "action": {
    "default_title": "Tab Navigator",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
          "commands": {
          "_execute_action": {
            "suggested_key": {
              "default": "Alt+Z",
              "mac": "Alt+Z"
            },
            "description": "Switch to most recent tab"
          }
        },
  

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
} 